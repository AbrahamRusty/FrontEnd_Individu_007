generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Course {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  level       String
  duration    Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}